{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  "baseUrl": "./src",
  "paths": {
    "@/kit/*": [
      "kit/*"
    ],
    "@/pages/*": [
      "pages/*"
    ],
    "@/routers/*": [
      "routers/*"
    ],
    "@/assets/*": [
      "assets/*"
    ],
    "@/redux/*": [
      "redux/*"
    ],
  },
}